	keywords = {Animals, Equipment Design, Equipment Failure Analysis, Magnetic Resonance Imaging, Magnetics, Mice, Reproducibility of Results, Sensitivity and Specificity, Transducers, Whole Body Imaging},
	pages = {584--595}
}

