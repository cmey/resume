	keywords = {Artifacts, Contrast Media, Coronary Disease, Gadolinium DTPA, Humans, Image Enhancement, Image Processing, Computer-Assisted, Magnetic Resonance Imaging, Myocardial Infarction, Myocardium, Phantoms, Imaging, Reference Values, Sensitivity and Specificity},
