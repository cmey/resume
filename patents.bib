	pmid = {20936461},
	keywords = {Algorithms, Cohort Studies, Contrast Media, Fiducial Markers, Humans, Male, Prostatic Neoplasms, Prostheses and Implants, Radiotherapy Planning, Computer-Assisted, Tomography, X-Ray Computed, X-Rays},
	pages = {544--550}
