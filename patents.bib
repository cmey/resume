	number = {1},
	urldate = {2011-10-30},
	journal = {Journal of Magnetic Resonance Imaging},
	author = {Wood, Michael L and Shivji, Mansur J and Stanchev, Peter L},
	month = jan,
	year = {1995},
	keywords = {Artifact, Image processing, K‐space, motion correction},
	pages = {57--64}
}

