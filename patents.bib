	keywords = {Adult, Female, Heart Diseases, Humans, Imaging, Three-Dimensional, Magnetic Resonance Imaging, Cine, Male, Middle Aged, Motion, Myocardial Contraction, Phantoms, Imaging, Prospective Studies},
	pages = {917--925}
}

