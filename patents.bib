	keywords = {Adult, Blood Pressure, Coronary Angiography, Coronary Circulation, Female, Heart Transplantation, Heart rate, Humans, Magnetic Resonance Angiography, Male, Nitrogen Radioisotopes, Reference Values, Tomography, Emission-Computed, Ventricular Function, Left},
