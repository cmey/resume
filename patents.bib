	pmid = {20432355},
	keywords = {Adult, Aorta, Thoracic, Blood Flow Velocity, Contrast Media, Female, Fourier Analysis, Humans, Image Enhancement, Image Interpretation, Computer-Assisted, Magnetic Resonance Imaging, Cine, Male, Middle Aged, Perfusion Imaging, Pulsatile Flow, Reproducibility of Results, Rheology, Sensitivity and Specificity},
