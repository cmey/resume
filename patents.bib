	keywords = {Adolescent, Adult, Aged, Aged, 80 and over, Analysis of Variance, Case-Control Studies, Female, Heart Diseases, Humans, Image Enhancement, Image Processing, Computer-Assisted, Imaging, Three-Dimensional, Magnetic Resonance Imaging, Cine, Male, Middle Aged, Statistics, Nonparametric},
