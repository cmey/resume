@article{markl_phase_2001-2,
	title = {Phase contrast {MRI} with improved temporal resolution by view sharing: k-space related velocity mapping properties},
	volume = {19},
	issn = {0730-725X},
	shorttitle = {Phase contrast {MRI} with improved temporal resolution by view sharing},
	abstract = {Phase contrast techniques in combination with k-space segmented CINE imaging are widely used for the quantitative assessment of blood flow or tissue motion. The temporal resolution of the corresponding pulse sequences plays an important role concerning the potential of the method to fully detect time resolved flow or motion patterns. A further improvement of temporal or spatial resolution in phase contrast CINE MRI can be achieved by the application of view sharing. Based on simulations with point-spread-functions resulting from different cyclic flow or motion patterns an analysis of view sharing techniques in combination with phase contrast MRI is presented. Velocity mapping properties and the role of different k-space regions concerning the resulting values in the phase images and thus encoded velocities were investigated. It could be shown that the velocity induced phase shifts in phase contrast techniques are mainly encoded in the central sections of k-space which makes view sharing also suitable for velocity mapping. As a result the use of appropriate sampling and data acquisition schemes permits the assessment of flow or motion patterns with significantly improved temporal resolution without loss of functional information. In addition phantom measurements with an oscillation phantom were performed in order to validate the simulation results and to demonstrate the potential of view sharing techniques to accelerate phase contrast imaging and improve the detection of the underlying flow or motion dynamics.},
	language = {eng},
	number = {5},
	journal = {Magnetic Resonance Imaging},
	author = {Markl, M. and Hennig, J.},
	month = jun,
	year = {2001},
	pmid = {11672625},
	keywords = {Blood Flow Velocity, Computer Simulation, Coronary Circulation, Fourier Analysis, Humans, Image Enhancement, Magnetic Resonance Imaging, Cine, Models, Cardiovascular, Myocardial Contraction, Phantoms, Imaging},
	pages = {669--676}
}

@article{meyer_snr-enhanced_2014,
	title = {{SNR}-enhanced cardiac cine {MRI} in small animals using motion correction},
	abstract = {Object: Retrospectively gated cardiac MRI of small animals is limited by SNR. We propose a method allowing 100\% acquisition efficiency and large cardiac window widths to enhance SNR. Non-rigid respiratory and cardiac motion is corrected by the reconstruction.
Materials and Methods: Ten rats were subjected to sham surgery (n=3) or transient ischemia for 45 minutes followed by reperfusion (n=7). A retrospectively gated cardiac MRI sequence, IntraGate FLASH (Bruker BioSpin MRI GmbH, Ettlingen), was used for cine MRI in short axis. Navigator echoes provided input signals for the motion corrected reconstruction.
Results: SNR was increased by a factor of 1.6 while left ventricular end-diastolic volume, end-systolic volume, ejection fraction were not significantly different. End-diastolic mass was significantly different and closer to the true mass.
Conclusion: Retrospectively gated cine MRI can benefit from motion correction to increase SNR. Because cardiac motion is compensated for in the reconstruction, the cardiac window used for reconstruction can be increased to include more data and this leads to increased SNR.},
	journal = {submitted},
	author = {Meyer, Christophe and Nicol, Lionel and Odille, Freddy and Thuillez, Christian and Felblinger, Jacques and Vuissoz, Pierre-André},
	year = {2014}
}

@article{ratering_accelerated_2010,
