	urldate = {2012-12-10},
	journal = {Circulation},
	author = {Hundley, W. Gregory and Lange, Richard A. and Clarke, Geoffrey D. and Meshack, Benjamin M. and Payne, Jerry and Landau, Charles and McColl, Roderick and Sayad, Dany E. and Willett, DuWayne L. and Willard, John E. and Hillis, L. David and Peshock, Ronald M.},
	month = apr,
	year = {1996},
	keywords = {Coronary Disease, Magnetic Resonance Imaging, Regional Blood Flow},
	pages = {1502--1508}
}

