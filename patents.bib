@article{meyer_subject-specific_2014,
	title = {Subject-specific calibration of end-systolic time in {MRI}},
	abstract = {Purpose	A cardiac model is required either to predict a cardiac time period in which triggered data can be acquired with minimal motion, or to associate acquired k-space lines with cardiac phases in retrospective reconstruction of cine acquisitions. The usual timing model is based on the prediction of systole durations from the heart rate. It was calibrated on a population yielding fixed parameters and cannot account for individual variability. To improve high resolution MRI, we propose a personalized model that better predicts systole.
Methods	A phase contrast sequence was modified to acquire only central k-space line with 6.6 ms temporal resolution. Blood velocity was measured in aorta during 128 heartbeats in 35 subjects. Segmentation of aorta and detection of end of forward systolic ejection was automatic. Duration of electromechanical systole as function of heart rate was determined for each subject separately.
Results		The adapted cardiac model predicted significantly better both MRI end-systolic times and echocardiographic reference measurements (6.7 ms average absolute error, close to temporal resolution of acquisition) compared to global model (16.4 ms average absolute error). Favourable impact on cine reconstruction was shown.
Conclusion	Personalization of cardiac model to the subject is feasible in MRI and reduces the error of prediction of systole.},
	journal = {submitted},
	author = {Meyer, Christophe and Bonnemains, Laurent and Huttin, Olivier and Felblinger, Jacques and Vuissoz, Pierre-André},
	year = {2014}
}

