	issn = {1522-2594},
	doi = {10.1002/mrm.22615},
	abstract = {The temporal and spatial resolution of real-time phase-contrast magnetic resonance (PCMR) is restricted by the need to acquire two interleaved phase images. In this article, we propose a split-acquisition real-time CINE PCMR technique, where the acquisition of flow-encoded and flow-compensated data is divided into separate blocks. By comparing magnitude images, automatic matching of data in cardio-respiratory space allows subtraction of background phase offsets. Thus, the data is acquired in real-time but with phase correction originating from a different heart beat. This effectively doubles the frame rate, allowing either higher temporal or spatial resolution. Two split-acquisition sequences were tested: one with high-temporal resolution and one with high-spatial resolution. Both sequences showed excellent agreement in stroke volumes in 20 adults when validated against cardiac-gated PCMR and interleaved real-time PCMR (cardiac gated: 95.2 ± 20.0 mL, interleaved real-time: 96.2 ± 20.7 mL, high-temporal resolution: 95.6 ± 20.1 mL, high-spatial resolution: 95.5 ± 20.4 mL). In six children, the high-spatial resolution sequence provided more accurate flow measurements than interleaved real-time PCMR, when compared with cardiac-gated PCMR (cardiac gated: 20.6 ± 7.6 mL, interleaved real-time: 24.3 ± 9.2 mL, high-spatial resolution: 20.8 ± 7.8 mL), due to the increased spatial resolution. The matching technique is shown to be accurate (truth: 94.6 ± 21.8, split-acquisition: 95.0 ± 21.9 mL) and quantitative image quality (signal-to-noise ratio, velocity-to-noise ratio and edge sharpness) is acceptable.},
	language = {eng},
	number = {6},
	journal = {Magnetic Resonance in Medicine: Official Journal of the Society of Magnetic Resonance in Medicine / Society of Magnetic Resonance in Medicine},
	author = {Steeden, Jennifer A. and Atkinson, David and Taylor, Andrew M. and Muthurangu, Vivek},
	month = dec,
	year = {2010},
	pmid = {20939086},
	keywords = {Adolescent, Adult, Feasibility Studies, Female, Heart Defects, Congenital, Humans, Image Enhancement, Image Interpretation, Computer-Assisted, Magnetic Resonance Imaging, Cine, Male},
	pages = {1664--1670}
}

