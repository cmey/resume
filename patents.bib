	keywords = {Angiography, Magnetic Resonance Imaging, coronary arteries, heart rate variability, comparative studies},
	pages = {577--582},
	file = {Snapshot:/Users/cmey/Zotero/storage/5WH5XKQ6/abstract.html:text/html}
