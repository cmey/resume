	author = {Golde, David and Burstin, Luis},
	month = dec,
	year = {1970},
	pmid = {5492536},
	keywords = {Apexcardiogram, Ejection time, Electromechanical delay, Isometric contraction time, Phonocardogram, Polycardiography, Pre-ejection period, Systolic time intervals},
