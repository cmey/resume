	journal = {Journal of magnetic resonance imaging: JMRI},
	author = {Sakuma, H and Globits, S and O'Sullivan, M and Shimakawa, A and Bernstein, M A and Foo, T K and Amidon, T M and Takeda, K and Nakagawa, T and Higgins, C B},
