	pmid = {19519892},
	keywords = {Adult, Blood Flow Velocity, Coronary Sinus, Female, Heart Ventricles, Humans, Image Interpretation, Computer-Assisted, Magnetic Resonance Imaging, Cine, Male, Middle Aged, Perfusion Imaging, Phantoms, Imaging, Ventricular Function, Left, Young Adult},
	pages = {9}
}

