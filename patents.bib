@article{bloch_nuclear_1946,
	title = {Nuclear induction},
	volume = {17},
	issn = {0031-8914},
	url = {http://www.sciencedirect.com/science/article/B6X42-4CRP4NT-5H/2/35feeef8accacf86e587fd7110229b3b},
	doi = {10.1016/0031-8914(51)90068-7},
	abstract = {The investigation of nuclear moments by nuclear induction or, as it is sometimes called, magnetic resonance absorption has progressed in many laboratories and in a variety of directions since its initiation. We shall report upon two special developments in which we have recently been most interested.
The first is the establishment of suitable standards of comparison for the measurement of magnetic moments. While, until recently, the magnetic moment of the proton has been used as standard, there are now measurements carried out which give directly and with high precision the magnetic moment of the proton in units of the nuclear magneton. They are based upon the comparison of its resonance frequency with the orbital frequency of protons, circulating in the same magnetic field. The latter frequency has been measured by observing either the accelerating or the decelerating action of an applied electric field at radio frequencies. Both methods will be discussed and results will be presented.
The second development is the application of nuclear induction to a great variety of different nuclei. The proper choice in the chemical composition of the sample is here of major importance in order to obtain suitable concentration, relaxation time and line width. Particularly in cases where the nucleus possesses a large electric quadrupole moment, it is important to find a compound such that the broadening electric field gradients are greatly reduced by the symmetry of the molecule. It has been found, in this connection, that different compounds can lead to different resonance frequencies for the same nucleus, the largest differences, sofar observed, being that of certain cobalt compounds and amounting to more than one percent. A possible interpretation of these chemical effects will be presented.},
	number = {3-4},
	urldate = {2011-03-08},
	journal = {Physica},
	author = {Bloch, F.},
	year = {1946},
	pages = {272--281}
}

@article{clique_3d_2014,
	title = {3D myocardial {T}1 mapping at 3T using variable flip angle method: {Pilot} study},
